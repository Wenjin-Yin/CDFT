&GLOBAL
  RUN_TYPE  GEO_OPT
  PRINT_LEVEL MEDIUM
&END GLOBAL

&MOTION
 &GEO_OPT
   MAX_ITER  1000
   MAX_DR    1.E-3
   RMS_DR    3.E-4
   MAX_FORCE 1.E-4
   RMS_FORCE 3.E-5
   OPTIMIZER BFGS
 &END GEO_OPT
&END MOTION

&FORCE_EVAL
 &DFT
   BASIS_SET_FILE_NAME  BASIS_SET 
   POTENTIAL_FILE_NAME  GTH_POTENTIALS
UKS T
  MULTIPLICITY 2
  CHARGE -1
  PLUS_U_METHOD LOWDIN
 &MGRID
   NGRIDS 5
  &RS_GRID
  &END RS_GRID
   CUTOFF 500
   COMMENSURATE
  &END MGRID
  &QS
    EXTRAPOLATION ASPC
    EXTRAPOLATION_ORDER 3
    EPS_DEFAULT 1.0E-12
   &CDFT 
     TYPE_OF_CONSTRAINT Hirshfeld 
     ATOMIC_CHARGES T 
     STRENGTH 0 
     TARGET 1
    &ATOM_GROUP 
      ATOMS 18  
      COEFF 1 
      CONSTRAINT_TYPE  MAGNETIZATION 
     &END ATOM_GROUP
     &OUTER_SCF 
       TYPE CDFT_CONSTRAINT 
       EPS_SCF 1E-3 
       OPTIMIZER SECANT
       STEP_SIZE 0.05 
     &END OUTER_SCF
     &HIRSHFELD_CONSTRAINT
       SHAPE_FUNCTION DENSITY 
     &END HIRSHFELD_CONSTRAINT
    &END CDFT
  &END QS
  &SCF
    EPS_SCF 5.0E-7
    MAX_SCF 100
    SCF_GUESS RESTART 
   &OUTER_SCF
     MAX_SCF 20
     OPTIMIZER DIIS
     STEP_SIZE 0.15
     EPS_SCF 5.0E-7
   &END OUTER_SCF
   &OT T 
    MINIMIZER CG
    ALGORITHM IRAC
    PRECONDITIONER FULL_ALL
    ENERGY_GAP 0.001
   &END OT
  &END SCF
  &PRINT
  &END PRINT
  &XC
   &XC_FUNCTIONAL PBE
   &END XC_FUNCTIONAL
   &VDW_POTENTIAL
     POTENTIAL_TYPE PAIR_POTENTIAL
    &PAIR_POTENTIAL
      TYPE DFTD3
      PARAMETER_FILE_NAME dftd3.dat
      REFERENCE_FUNCTIONAL PBE
    &END PAIR_POTENTIAL
   &END VDW_POTENTIAL
   &XC_GRID
     XC_SMOOTH_RHO NN10 
     XC_DERIV SPLINE2_smooth
   &END XC_GRID
  &END XC
 &END DFT
 &SUBSYS
  &CELL
   ABC 12.9938   11.8360   25.0000
  &END CELL

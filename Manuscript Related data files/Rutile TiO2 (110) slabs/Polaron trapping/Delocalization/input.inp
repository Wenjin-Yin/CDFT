@SET OT 1
@SET TD 0

&GLOBAL
  PROJECT  TiO2-rutile-110
  RUN_TYPE  energy 
  PRINT_LEVEL medium
&END GLOBAL

&MOTION
  &GEO_OPT
    MAX_ITER 40000
    MAX_DR 1.E-3	
    MAX_FORCE 1.E-4
    OPTIMIZER BFGS
  &END GEO_OPT
&END MOTION

&FORCE_EVAL
&DFT
  BASIS_SET_FILE_NAME    BASIS_SET
  POTENTIAL_FILE_NAME    GTH_POTENTIALS
CHARGE -1
MULTIPLICITY 2
UKS T

 PLUS_U_METHOD LOWDIN
    &MGRID
    &RS_GRID
     &END RS_GRID
     CUTOFF 500
     COMMENSURATE
   &END MGRID
  &PRINT
    &PDOS
     NLUMO 500
    &END PDOS
     &MO_CUBES
      NHOMO 2
      NLUMO 2
     &END MO_CUBES
    &END PRINT

&QS
  EXTRAPOLATION PS
  EXTRAPOLATION_ORDER 3
  EPS_DEFAULT 1.0E-12
&END QS
&SCF
  @IF ${TD}
    EPS_SCF 5.0E-7
    MAX_SCF 300
    SCF_GUESS RESTART
   &MIXING on
     ALPHA 0.2
    &END MIXING
  @ENDIF
  @IF ${OT}
    EPS_SCF 5.0E-7
    MAX_SCF 200
    SCF_GUESS RESTART
   &OT on
     MINIMIZER DIIS
     PRECONDITIONER FULL_ALL
     ENERGY_GAP 0.01
   &END OT
   &OUTER_SCF on
     EPS_SCF 5.0E-7
     MAX_SCF 100
   &END OUTER_SCF
   @ENDIF
    &PRINT
     &RESTART
       BACKUP_COPIES 0
      &EACH
        QS_SCF 10
      &END EACH
     &END RESTART
    &END PRINT
   &END SCF
    &XC
      &XC_FUNCTIONAL PBE
       &END XC_FUNCTIONAL
      &VDW_POTENTIAL
        POTENTIAL_TYPE PAIR_POTENTIAL
       &PAIR_POTENTIAL
         TYPE DFTD3
         PARAMETER_FILE_NAME dftd3.dat
         REFERENCE_FUNCTIONAL PBE
        &END PAIR_POTENTIAL
      &END VDW_POTENTIAL
      &XC_GRID
        XC_DERIV SPLINE2_smooth
      &END XC_GRID
     &END XC
  &END DFT

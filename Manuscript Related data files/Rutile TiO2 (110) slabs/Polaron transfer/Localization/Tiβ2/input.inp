&GLOBAL
  RUN_TYPE  GEO_OPT 
  PRINT_LEVEL MEDIUM
&END GLOBAL

&MOTION
 &GEO_OPT
   MAX_ITER  1000
   MAX_DR    1.E-3
   RMS_DR    3.E-4
   MAX_FORCE 1.E-4
   RMS_FORCE 3.E-5
   OPTIMIZER BFGS
 &END GEO_OPT
&END MOTION

&FORCE_EVAL
 &DFT
   BASIS_SET_FILE_NAME  BASIS_SET 
   POTENTIAL_FILE_NAME  GTH_POTENTIALS
UKS T
  MULTIPLICITY 2
  CHARGE -1
  PLUS_U_METHOD LOWDIN
 &MGRID
   NGRIDS 5
  &RS_GRID
  &END RS_GRID
   CUTOFF 500
   COMMENSURATE
  &END MGRID

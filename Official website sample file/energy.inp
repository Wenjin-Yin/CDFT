@SET RESTART_1            EXTERNAL_RESTART_WFN_1
@SET WFN_FILE_1           EXTERNAL_WFNRESTART_1

@SET DFT_FILE             dft-common-params.inc

@SET PROJECT_NAME         EXTERNAL_PROJNAME
@SET XYZFILE              EXTERNAL_XYZNAME

@SET PW_CUTOFF            300
@SET PW_REL_CUTOFF        50

@SET CONSTR_ACTIVE        EXTERNAL_CONSTR_ACTIVE
@SET CONSTR_STR_1         EXTERNAL_CONSTR_STR1
@SET CONSTR_TARGET_1      EXTERNAL_CONSTR_TARGET1

@SET HIRSHFELD_SHAPEFN    EXTERNAL_SHAPEFN
@SET GAUSSIAN_SHAPE       EXTERNAL_GAUSSIAN


&GLOBAL
  PROJECT ${PROJECT_NAME}
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL

&FORCE_EVAL
  @SET WFN_FILE     ${WFN_FILE_1}
  @SET RESTART      ${RESTART_1}
  @SET NAME         ${PROJECT_NAME}
  @SET CONSTR_TARGET ${CONSTR_TARGET_1}
  @SET CONSTR_STR    ${CONSTR_STR_1}
  METHOD QS
  @include ${DFT_FILE}
  @include subsys.inc
&END FORCE_EVAL
